@ECHO OFF

SET WEBDIR=#WEBDIR#
SET PHPDIRTS=#PHPDIRTS#
SET PHPDIRNTS=#PHPDIRNTS#

CD "%WEBDIR%"

IF EXIST "%WEBDIR%\composer.lock" ( DEL "%WEBDIR%\composer.lock" /F /Q )

"%PHPDIRTS%\php.exe" "%PHPDIRTS%\composer.phar" update --no-dev --prefer-dist
REM "%PHPDIRNTS%\php.exe" "%PHPDIRNTS%\composer.phar" update --no-dev --prefer-dist

ECHO.
ECHO Done

PAUSE